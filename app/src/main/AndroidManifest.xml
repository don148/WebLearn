<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.beta.android.aakashresearchlabs.test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="WebLearn"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="WebLearn"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lessons.BasicActivity"
            android:label="Basic"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
        <activity android:name=".lessons.GitBasicActivity"
            android:label="Git Basic"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
        <activity android:name=".lessons.FrontEndActivity"
            android:label="Front End"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
        <activity android:name=".lessons.BackEndActivity"
            android:label="Back End"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
        <activity android:name=".lessons.FrameworkActivity"
            android:label="Framework"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
        <activity android:name=".lessons.ImportantActivity"
            android:label="Important"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
        <activity android:name=".lessonActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
        <activity android:name=".TestActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/itemMenu"
            />
    </application>

</manifest>